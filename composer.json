{
    "name": "friendsofvictoire/text-widget",
    "type": "symfony-bundle",
    "description": "Victoire Cms Text component",
    "keywords": ["cms", "victoire", "text"],
    "homepage": "http://appventus.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Andrieux",
            "email": "paul@appventus.com"
        }
    ],
    "require": {
        "php"                                 : ">=5.3.3",
        "symfony/framework-bundle"            : "~2.3",
        "victoire/victoire"                   : "dev-master"
    },
    "autoload": {
        "psr-0": { "Victoire\\Widget\\TextBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Victoire/Widget/TextBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
